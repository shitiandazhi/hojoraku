<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-6">
      <%= form_with model: @user, url: user_session_path(@user), local: true do |f| %>

        <h4>法人形式</h4>
        <%= f.text_field :company_from, class: "form-control" %>
        <h4>法人名</h4>
        <%= f.text_field :name, class: "form-control" %>
        <h4>フリガナ</h4>
        <%= f.text_field :name_kana, class: "form-control" %>
        <h4>法人番号</h4>
        <%= f.text_field :company_number, class: "form-control" %>
        <h4>郵便番号</h4>
        <%= f.text_field :post_code, class: "form-control" %>
        <h4>住所</h4>
        <%= f.text_field :address, class: "form-control" %>
        <h4>電話番号</h4>
        <%= f.text_field :phonenumber, class: "form-control" %>
        <h4>メールアドレス</h4>
        <%= f.text_field :email, class: "form-control" %>

        <div class="row mt-4 pb-5">
          <div class="form-inline mx-auto">
            <%= f.submit '変更を保存', class:"btn btn-success ml-5" %>
            <%= link_to "退会する", users_check_users_path,class:"btn btn-danger ml-3" %>
          </div>
        </div>

      <% end %>
    </div>
  </div>
</div>