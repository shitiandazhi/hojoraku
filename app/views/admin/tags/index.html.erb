タグ一覧・追加
<%= form_with model: @tag,url: admin_tags_path do |f| %>
  <%= f.label "タグ名" %>
  <%= f.text_field :name %>
  <%= f.submit "新規登録", class:"btn btn-success" %>
<% end %>

<table class='table mt-5 w-50' style="color: white;">
  <thead style="background-color:#d3d3d3;">
    <tr>
      <th>タグ</th>
      <th colspan="3"></th>
    </tr>
  </thead>
 <% @tags.each do |tag| %>
    <tbody>
      <th><%= tag.name %></th>
      <th><%= link_to "編集する", edit_admin_tag_path(tag), class:"btn btn-success py-0" %></th>
    </tbody>
  <% end %>
</table>